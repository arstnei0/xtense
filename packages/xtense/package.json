{
	"name": "xtense",
	"version": "0.0.7",
	"scripts": {
		"dev": "vite",
		"lint": "eslint . --ext .ts",
		"format": "prettier . --write",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"build": "tsup"
	},
	"devDependencies": {
		"@types/node": "^18.8.3",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"eslint": "^8.24.0",
		"prettier": "^2.7.1",
		"vite": "^3.1.4",
		"vitest": "^0.23.4"
	},
	"dependencies": {
		"typescript": "^4.6.4"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"type": "module"
}
